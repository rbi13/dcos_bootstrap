{
  "id": "touch-test",
  "description": "touch to test mounts",
  "run": {
    "cmd": "touch /output/b",
    "cpus": 0.01,
    "mem": 32,
    "disk": 0,
    "docker": {
      "image": "alpine"
    },
    "volumes": [
        {
            "containerPath": "/output",
            "hostPath": "/tmp",
            "mode": "RW"
        }
    ]
  },
  "schedules": []
}